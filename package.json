{
    "name": "pelindo-biodata-backend",
    "version": "1.0.0",
    "description": "Backend API for Pelindo Biodata Application",
    "main": "app.js",
    "scripts": {
        "start": "node bin/www",
        "dev": "nodemon bin/www",
        "test": "jest",
        "db:migrate": "sequelize-cli db:migrate",
        "db:seed": "sequelize-cli db:seed:all",
        "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
    },
    "dependencies": {
        "bcryptjs": "^3.0.2",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "ejs": "^3.1.10",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.16.3",
        "sequelize": "^6.37.7",
        "zod": "^3.25.74"
    },
    "devDependencies": {
        "sequelize-cli": "^6.6.3",
        "nodemon": "^3.0.0"
    }
}
